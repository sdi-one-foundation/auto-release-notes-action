{
  "type": "message",
  "text": "Single Digits Release Documentation",
  "attachments": [
    {
      "contentType": "application/vnd.microsoft.card.adaptive",
      "content": {
        "type": "AdaptiveCard",
        "version": "1.0",
        "body": [
          {
            "type": "TextBlock",
            "text": "Release Notes for <Name of Service>",
            "size": "large"
          },
          {
            "type": "TextBlock",
            "text": "Team : <Team Name>"
          },
          {
            "type": "TextBlock",
            "text": "Version : <Version>"
          },
          {
            "type": "TextBlock",
            "text": "Date Released : <Date Released>"
          }
        ],
        "actions": [
          {
            "type": "Action.OpenUrl",
            "url": "<Repository URL>/blob/main/CHANGELOG.md",
            "title": "Release Notes"
          }
        ]
      }
    }
  ]
}
